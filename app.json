{
  "expo": {
    "name": "Instagram Story Clone",
    "slug": "instagramstoryclone",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Instagram story clone with expo av",
    "githubUrl": "https://github.com/codingki/instagram-story-react-native-expo"
  }
}
